{
  "name": "projects-browser",
  "displayName": "Projects Browser",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:projectsBrowser"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "test.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "_project-browser.openInNewWindow",
        "title": "Open in new Window",
        "icon": "$(link-external)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "projects-browser",
          "title": "Projects Browser",
          "icon": "$(folder-library)"
        }
      ]
    },
    "views": {
      "projects-browser": [
        {
          "id": "projectsBrowser",
          "name": "Projects Browser"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "_project-browser.openInNewWindow",
          "when": "view == projectsBrowser && viewItem == 'repository'",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}
